<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks" android:key="@string/category_key_wireless">
        <dashboard-tile android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/wifi_settings_title" android:fragment="com.android.vendorsettings.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_bluetooth" android:id="@id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.vendorsettings.bluetooth.BluetoothSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_sim" android:id="@id/sim_settings" android:title="@string/sim_settings_title" android:fragment="com.android.vendorsettings.sim.SimSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/data_usage_settings" android:title="@string/data_usage_summary_title" android:fragment="com.android.vendorsettings.DataUsageSummary" />
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.vendorsettings.WirelessSettings">
            <intent android:action="com.android.vendorsettings.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_more" android:id="@id/wireless_settings" android:title="@string/radio_controls_title" android:fragment="com.android.vendorsettings.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/header_category_device" android:key="@string/category_key_device">
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@id/home_settings" android:title="@string/home_settings" android:fragment="com.android.vendorsettings.HomeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.vendorsettings.DisplaySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/notification_settings" android:fragment="com.android.vendorsettings.notification.NotificationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/applications_settings" android:fragment="com.android.vendorsettings.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/storage_usb_settings" android:fragment="com.android.vendorsettings.deviceinfo.StorageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.vendorsettings.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_memory" android:id="@id/manage_memory" android:title="@string/memory_settings_title" android:fragment="com.android.vendorsettings.applications.ProcessStatsSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.vendorsettings.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.vendorsettings.nfc.PaymentSettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.vendorsettings.WirelessSettings">
            <intent android:action="com.android.vendorsettings.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/personal_section" android:title="@string/header_category_personal" android:key="@string/category_key_personal">
        <dashboard-tile android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/location_settings_title" android:fragment="com.android.vendorsettings.location.LocationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_security" android:id="@id/security_settings" android:title="@string/security_settings_title" android:fragment="com.android.vendorsettings.SecuritySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_accounts" android:id="@id/account_settings" android:title="@string/account_settings_title" android:fragment="com.android.vendorsettings.accounts.AccountSettings" />
        <external-tiles />
        <dashboard-tile android:icon="@drawable/ic_settings_language" android:id="@id/language_settings" android:title="@string/language_settings" android:fragment="com.android.vendorsettings.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.vendorsettings.PrivacySettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_category_system" android:key="@string/category_key_system">
        <dashboard-tile android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.vendorsettings.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.vendorsettings.accessibility.AccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" android:fragment="com.android.vendorsettings.print.PrintSettingsFragment" />
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.vendorsettings.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.vendorsettings.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>